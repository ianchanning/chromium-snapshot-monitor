{
	"name": "Chromium Snapshot Monitor",
	"version": "1.13",
	"description": "Monitors the latest snapshot releases",
	"background_page": "background.html",
	"options_page": "options.html",
	"icons": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "48": "icon-48.png",
      "64": "icon-64.png",
      "128": "icon-128.png",
      "256": "icon-256.png",
      "512": "icon-512.png"
   	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Chromium Snapshot Monitor",
		"popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"http://build.chromium.org/"
	]
}
